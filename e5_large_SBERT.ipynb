        "def normalize_category(cat: str) -> str:\n",
        "    if not cat:\n",
        "        return \"\"\n",
        "    cat = cat.replace(\"_\", \" \") # Erstatter underscore med mellemrum (eks. NDK_StoreEquipment_PDA_Bluebird20 bliver til NDK StoreEquipment PDA BluebirdS20)\n",
        "    cat = re.sub(r\"([a-z])([A-Z])\", r\"\\1 \\2\", cat)  # Indsætter mellemrum før store bogstaver (eks. BluebirdS20 bliver til Bluebird S20)\n",
        "    return cat"
      ],
      "metadata": {
        "id": "1nh2rLlC0yoD"
      },
      "execution_count": 4,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def build_passage(row):\n",
        "    category = normalize_category(row.get(\"subcategory\", \"\"))\n",
        "    second_category = normalize_category(row.get(\"u_3rd_category\", \"\"))\n",
        "    parts = [\"passage:\"]\n",
        "\n",
        "    if category:\n",
        "        parts.append(f\"Category: {category}\")\n",
        "\n",
        "    if second_category:\n",
        "        parts.append(f\"3rd Category: {second_category}\")\n",
        "\n",
        "    if row[\"short_description\"]:\n",
        "        parts.append(f\"Title: {row['short_description']}\")\n",
        "\n",
        "    if row[\"description\"]:\n",
        "        parts.append(f\"Description: {row['description']}\")\n",
        "\n",
        "    if row[\"close_notes\"]:\n",
        "        parts.append(f\"Resolution: {row['close_notes']}\")\n",
        "\n",
        "    return \"\\n\".join(parts)"
      ],
      "metadata": {
        "id": "4B76OtTc0z0L"
      },
      "execution_count": 5,
      "outputs": []
    },
    {
      "cell_type": "code",
