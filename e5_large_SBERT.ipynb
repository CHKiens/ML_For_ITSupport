      "source": [
        "def search_tickets(query, top_k=5):\n",
        "    query_embedding = model.encode(\n",
        "        f\"query: {query}\",\n",
        "        normalize_embeddings=True\n",
        "    )\n",
        "\n",
        "    # cosinus lighed (dot produkt da embeddings er normaliserede)\n",
        "    scores = np.dot(ticket_embeddings, query_embedding)\n",
        "\n",
        "    top_indices = np.argsort(scores)[::-1][:top_k]\n",
        "\n",
        "    results = []\n",
        "    for idx in top_indices:\n",
        "        row = df.iloc[idx]\n",
        "        results.append({\n",
        "            \"ticket_id\": row[\"number\"],\n",
        "            \"score\": float(scores[idx]),\n",
        "            \"second_category\": row.get(\"subcategory\", \"\"),\n",
        "            \"category\": row.get(\"u_3rd_category\", \"\"),\n",
        "            \"title\": row[\"short_description\"],\n",
        "            \"description\": row[\"description\"],\n",
        "            \"resolution_preview\": row[\"close_notes\"][:300]\n",
        "        })\n",
        "\n",
        "    return results"
      ],
